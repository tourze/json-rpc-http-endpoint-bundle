{
  "name": "tourze/json-rpc-http-endpoint-bundle",
  "description": "JsonRPC HTTP入口模块",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "symfony/config": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-foundation": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/property-access": "^7.3",
    "symfony/routing": "^7.3",
    "symfony/service-contracts": "^3.6",
    "symfony/yaml": "^7.3",
    "tourze/backtrace-helper": "1.*",
    "tourze/bundle-dependency": "1.*",
    "tourze/json-rpc-core": "1.0.*",
    "tourze/json-rpc-endpoint-bundle": "0.1.*",
    "tourze/symfony-dependency-service-loader": "1.0.*",
    "tourze/symfony-routing-auto-loader-bundle": "1.0.*",
    "willdurand/jsonp-callback-validator": "^2.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/phpunit-symfony-kernel-test": "1.0.*",
    "tourze/phpunit-symfony-unit-test": "1.*",
    "tourze/phpunit-symfony-web-test": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\JsonRPCHttpEndpointBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\JsonRPCHttpEndpointBundle\\Tests\\": "tests"
    }
  }
}